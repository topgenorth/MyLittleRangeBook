{
    "Sqlite": {
        "DataDirectory": "~/.mylittlerangebook"
    },
    "Serilog": {
        "MinimumLevel": "Warning",
        "WriteTo": [
            {
                "Name": "Console"
            }
        ],
        "Enrich": [
            "FromLogContext",
            "WithMachineName"
        ],
        "Properties": {
            "ApplicationName": "xeror"
        }
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "System": "warning",
            "Microsoft": "Warning"
        }
    }
}
